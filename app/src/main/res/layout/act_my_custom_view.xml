<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/backLayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_71dp"
        android:background="@color/app_title_bar_color_v3">

        <ImageView
            android:id="@+id/gobackView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/margin_16dp"
            android:paddingRight="@dimen/margin_16dp"
            android:src="@mipmap/img_app_goback" />

        <TextView
            android:id="@+id/title_tv"
            style="@style/common_title_text"
            android:textColor="@color/app_common_content_v3" />

        <TextView
            android:id="@+id/tv_action_bar_right"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:gravity="center"
            android:paddingLeft="@dimen/margin_16dp"
            android:paddingRight="@dimen/margin_16dp"
            android:textColor="@color/app_common_content_v3"
            android:textSize="@dimen/text_size_14"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/iv_action_bar_right"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:paddingLeft="@dimen/margin_16dp"
            android:paddingRight="@dimen/margin_16dp"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_14"
            android:visibility="gone" />

    </FrameLayout>

    <com.example.view.CustomView
        android:id="@+id/ll_custom_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>